{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport http from 'axios';\nimport { bus } from '@/event-bus.js';\nexport default {\n  name: 'SignUpView',\n  components: {},\n  data() {\n    return {\n      user: {\n        id_usuario: 0,\n        nombre: '',\n        email: '',\n        passwd: ''\n      },\n      userName: '',\n      password: '',\n      isBad: false,\n      msg: ''\n    };\n  },\n  methods: {\n    checkUser() {\n      return http({\n        method: 'GET',\n        url: 'http://143.47.50.240:80/APIdotshare/usuarios'\n      }).then(response => response.data).then(users => {\n        this.user = users.filter(user => user.nombre === this.userName)[0];\n        if (!this.user) {\n          const errmsg = 'Usuario no existe';\n          bus.emit('mostarError', errmsg);\n        } else {\n          if (this.user.passwd !== this.password) {\n            const errmsg = 'Contraseña no coincide';\n            bus.emit('mostarError', errmsg);\n          } else {\n            console.log('Inicisaste sesion');\n            sessionStorage.setItem('user', JSON.stringify(this.user));\n            console.log('vue:' + this.user);\n            this.$router.push({\n              name: 'alperfil',\n              params: {\n                id: this.user.nombre\n              }\n            });\n            bus.emit('actualizaNav', false);\n            this.isBad = false;\n          }\n        }\n      }).catch(err => console.log(err));\n    }\n  }\n};","map":{"version":3,"names":["http","bus","name","components","data","user","id_usuario","nombre","email","passwd","userName","password","isBad","msg","methods","checkUser","method","url","then","response","users","filter","errmsg","emit","console","log","sessionStorage","setItem","JSON","stringify","$router","push","params","id","catch","err"],"sources":["C:\\Users\\mario\\OneDrive\\Escritorio\\TFG-DOTSHARE\\TFG-DAM-mainn\\src\\views\\SignIn.vue"],"sourcesContent":["<template>\n  <form @submit.prevent=\