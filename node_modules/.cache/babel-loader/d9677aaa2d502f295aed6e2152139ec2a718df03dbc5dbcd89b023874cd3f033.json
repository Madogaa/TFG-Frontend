{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport http from 'axios';\nimport { storage } from '@/vuefire/firebase';\nimport { ref, getDownloadURL } from '@firebase/storage';\nexport default {\n  name: 'ProfileSearch',\n  props: {\n    search: {\n      type: String\n    }\n  },\n  data() {\n    return {\n      views: []\n    };\n  },\n  methods: {\n    getListView() {\n      http({\n        method: 'GET',\n        url: `http://143.47.50.240:80/APIdotshare/perfillike/${this.search}`\n      }).then(response => response.data).then(response => {\n        response.forEach(e => {\n          http({\n            method: 'GET',\n            url: `http://143.47.50.240:80/APIdotshare/perfilbyid/${e.idPerfil}`\n          }).then(response => response.data).then(response => {\n            e.nick = response.nombre;\n            e.loaded = false;\n            getDownloadURL(ref(storage, `perfil/${e.nick}/${e.img}`)).then(url => {\n              e.loaded = true;\n              e.imageUrl = url;\n              this.views.push(e);\n            }).catch(error => {\n              e.loaded = false;\n              console.log('Error al descargar imagen:', error);\n              this.views.push(e);\n            });\n          });\n        });\n      });\n    }\n  },\n  watch: {\n    '$route.params': {\n      handler() {\n        this.views = [];\n        this.getListView();\n      },\n      immediate: true\n    }\n  }\n};","map":{"version":3,"names":["http","storage","ref","getDownloadURL","name","props","search","type","String","data","views","methods","getListView","method","url","then","response","forEach","e","idPerfil","nick","nombre","loaded","img","imageUrl","push","catch","error","console","log","watch","handler","immediate"],"sources":["C:\\Users\\mario\\OneDrive\\Escritorio\\TFG-DOTSHARE\\TFG-DAM-mainn\\src\\views\\ProfileSearch.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n        <router-link  v-for=\"view in views\" :key=\"view.idPerfil\" class=\"rlink\" :to=\"'/profile/'+view.nick\">\n          <img v-if=\"view.loaded\" class=\"profile__img\" :src=\"view.imageUrl\" alt=\"\">\n          <img v-if=\"!view.loaded\" class=\"profile__img\" src=\"../assets/default.png\" alt=\"\">\n          <div class=\"content\">\n             <h2>@{{ view.nick }}</h2>\n             <h5>{{ view.nombre }}</h5>\n             <p>{{ view.nseguidores }} followers</p>\n           </div>\n        </router-link>\n      </div>\n</template>\n\n<script>\nimport http from 'axios'\nimport { storage } from '@/vuefire/firebase'\nimport { ref, getDownloadURL } from '@firebase/storage'\nexport default {\n  name: 'ProfileSearch',\n  props: {\n    search: {\n      type: String\n    }\n  },\n  data () {\n    return {\n      views: []\n    }\n  },\n  methods: {\n    getListView () {\n      http(\n        {\n          method: 'GET',\n          url: `http://143.47.50.240:80/APIdotshare/perfillike/${this.search}`\n        }\n      )\n        .then((response) => response.data)\n        .then(response => {\n          response.forEach(e => {\n            http(\n              {\n                method: 'GET',\n                url: `http://143.47.50.240:80/APIdotshare/perfilbyid/${e.idPerfil}`\n              }\n            ).then((response) => response.data)\n              .then(response => {\n                e.nick = response.nombre\n                e.loaded = false\n                getDownloadURL(ref(storage, `perfil/${e.nick}/${e.img}`))\n                  .then(url => {\n                    e.loaded = true\n                    e.imageUrl = url\n                    this.views.push(e)\n                  })\n                  .catch(error => {\n                    e.loaded = false\n                    console.log('Error al descargar imagen:', error)\n                    this.views.push(e)\n                  })\n              })\n          })\n        })\n    }\n  },\n  watch: {\n    '$route.params': {\n      handler () {\n        this.views = []\n        this.getListView()\n      },\n      immediate: true\n    }\n  }\n}\n</script>\n\n<style scoped>\n.profile__img{\n  background: red;\n  width: 128px;\n  object-fit:cover;\n  border-radius: 50%;\n  height: 128px;\n}\n.content{\n  text-align: left;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  width: 270px;\n}\n\n.rlink{\n  padding: 1em;\n  width: 100%;\n  display: flex;\n  justify-content: center;\n  column-gap: 3em;\n  border-bottom: 1px solid gainsboro;\n  text-decoration: none;\n  color: #2c3e50;\n}\n.rlink:hover{\n  color: #2c3e50;\n}\n</style>\n"],"mappings":";AAeA,OAAOA,IAAG,MAAO,OAAM;AACvB,SAASC,OAAM,QAAS,oBAAmB;AAC3C,SAASC,GAAG,EAAEC,cAAa,QAAS,mBAAkB;AACtD,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,MAAM,EAAE;MACNC,IAAI,EAAEC;IACR;EACF,CAAC;EACDC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,WAAUA,CAAA,EAAK;MACbZ,IAAI,CACF;QACEa,MAAM,EAAE,KAAK;QACbC,GAAG,EAAG,kDAAiD,IAAI,CAACR,MAAO;MACrE,EACF,CACGS,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACP,IAAI,EAChCM,IAAI,CAACC,QAAO,IAAK;QAChBA,QAAQ,CAACC,OAAO,CAACC,CAAA,IAAK;UACpBlB,IAAI,CACF;YACEa,MAAM,EAAE,KAAK;YACbC,GAAG,EAAG,kDAAiDI,CAAC,CAACC,QAAS;UACpE,EACD,CAACJ,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACP,IAAI,EAC/BM,IAAI,CAACC,QAAO,IAAK;YAChBE,CAAC,CAACE,IAAG,GAAIJ,QAAQ,CAACK,MAAK;YACvBH,CAAC,CAACI,MAAK,GAAI,KAAI;YACfnB,cAAc,CAACD,GAAG,CAACD,OAAO,EAAG,UAASiB,CAAC,CAACE,IAAK,IAAGF,CAAC,CAACK,GAAI,EAAC,CAAC,EACrDR,IAAI,CAACD,GAAE,IAAK;cACXI,CAAC,CAACI,MAAK,GAAI,IAAG;cACdJ,CAAC,CAACM,QAAO,GAAIV,GAAE;cACf,IAAI,CAACJ,KAAK,CAACe,IAAI,CAACP,CAAC;YACnB,CAAC,EACAQ,KAAK,CAACC,KAAI,IAAK;cACdT,CAAC,CAACI,MAAK,GAAI,KAAI;cACfM,OAAO,CAACC,GAAG,CAAC,4BAA4B,EAAEF,KAAK;cAC/C,IAAI,CAACjB,KAAK,CAACe,IAAI,CAACP,CAAC;YACnB,CAAC;UACL,CAAC;QACL,CAAC;MACH,CAAC;IACL;EACF,CAAC;EACDY,KAAK,EAAE;IACL,eAAe,EAAE;MACfC,OAAMA,CAAA,EAAK;QACT,IAAI,CAACrB,KAAI,GAAI,EAAC;QACd,IAAI,CAACE,WAAW,EAAC;MACnB,CAAC;MACDoB,SAAS,EAAE;IACb;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencie