{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport http from 'axios';\nimport { storage } from '@/vuefire/firebase';\nimport { ref, getDownloadURL } from '@firebase/storage';\nexport default {\n  name: 'ProfileSearch',\n  props: {\n    search: {\n      type: String\n    },\n    id: {\n      type: String\n    }\n  },\n  data() {\n    return {\n      views: []\n    };\n  },\n  mounted() {\n    this.getListView();\n  },\n  methods: {\n    getImageUrl(view) {},\n    deleteSegui() {},\n    getListView() {\n      http({\n        method: 'GET',\n        url: `http://143.47.50.240:80/APIdotshare/perfil/${this.id}`\n      }).then(response => response.data).then(response => {\n        http({\n          method: 'GET',\n          url: `http://143.47.50.240:80/APIdotshare/perfil/${response.idPerfil}/${this.search}`\n        }).then(response => response.data).then(response => {\n          response.forEach(e => {\n            http({\n              method: 'GET',\n              url: `http://143.47.50.240:80/APIdotshare/perfilbyid/${e.idPerfil}`\n            }).then(response => response.data).then(response => {\n              e.nick = response.nombre;\n              e.loaded = false;\n              getDownloadURL(ref(storage, `perfil/${e.nick}/${e.img}`)).then(url => {\n                e.loaded = true;\n                e.imageUrl = url;\n                this.views.push(e);\n              }).catch(error => {\n                e.loaded = false;\n                console.log('Error al descargar imagen:', error);\n                this.views.push(e);\n              });\n            });\n          });\n        });\n      });\n    }\n  }\n};","map":{"version":3,"names":["http","storage","ref","getDownloadURL","name","props","search","type","String","id","data","views","mounted","getListView","methods","getImageUrl","view","deleteSegui","method","url","then","response","idPerfil","forEach","e","nick","nombre","loaded","img","imageUrl","push","catch","error","console","log"],"sources":["C:\\Users\\mario\\OneDrive\\Escritorio\\TFG-DOTSHARE\\TFG-DAM-mainn\\src\\views\\ProfileListView.vue"],"sourcesContent":["<template>\n    <div class=\"container\">\n      <div class=\"row\" v-for=\"view in views\" :key=\"view.idPerfil\">\n        <router-link  class=\"rlink col-11\" :to=\"'/profile/' + view.nick\">\n            <img v-if=\"view.loaded\" class=\"profile__img\" :src=\"view.imageUrl\" alt=\"\">\n            <img v-if=\"!view.loaded\" class=\"profile__img\" src=\"../assets/default.png\" alt=\"\">\n            <div class=\"content\">\n                <h2>@{{ view.nick }}</h2>\n                <h5>{{ view.nombre }}</h5>\n                <p>Followers: {{ view.nseguidores }}</p>\n            </div>\n        </router-link>\n        <div class=\"bin\" @click=\"deleteSegui()\">\n          <div class=\"h-26\">\n            <img src=\"../assets/borrar.png\" alt=\"\">\n          </div>\n        </div>\n      </div>\n    </div>\n</template>\n\n<script>\nimport http from 'axios'\nimport { storage } from '@/vuefire/firebase'\nimport { ref, getDownloadURL } from '@firebase/storage'\n\nexport default {\n  name: 'ProfileSearch',\n  props: {\n    search: {\n      type: String\n    },\n    id: {\n      type: String\n    }\n  },\n  data () {\n    return {\n      views: []\n    }\n  },\n  mounted () {\n    this.getListView()\n  },\n  methods: {\n    getImageUrl (view) {\n\n    },\n    deleteSegui(){\n\n    },\n    getListView () {\n      http(\n        {\n          method: 'GET',\n          url: `http://143.47.50.240:80/APIdotshare/perfil/${this.id}`\n        }\n      )\n        .then((response) => response.data)\n        .then(response => {\n          http(\n            {\n              method: 'GET',\n              url: `http://143.47.50.240:80/APIdotshare/perfil/${response.idPerfil}/${this.search}`\n            }\n          ).then((response) => response.data)\n            .then(response => {\n              response.forEach(e => {\n                http(\n                  {\n                    method: 'GET',\n                    url: `http://143.47.50.240:80/APIdotshare/perfilbyid/${e.idPerfil}`\n                  }\n                ).then((response) => response.data)\n                  .then(response => {\n                    e.nick = response.nombre\n                    e.loaded = false\n                    getDownloadURL(ref(storage, `perfil/${e.nick}/${e.img}`))\n                      .then(url => {\n                        e.loaded = true\n                        e.imageUrl = url\n                        this.views.push(e)\n                      })\n                      .catch(error => {\n                        e.loaded = false\n                        console.log('Error al descargar imagen:', error)\n                        this.views.push(e)\n                      })\n                  })\n              })\n            })\n        })\n    }\n  }\n}\n</script>\n\n  <style scoped>\n  .h-26{\n    align-self: center;\n    height: 26px;\n  }\n  .bin{\n    width: fit-content;\n    display: flex;\n    margin: auto;\n    height: 100%;\n  }\n  .bin:hover{\n    cursor: pointer;\n  }\n  .profile__img{\n    background: red;\n    width: 128px;\n    object-fit:cover;\n    border-radius: 50%;\n    height: 128px;\n  }\n  .content{\n    text-align: left;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    width: 270px;\n  }\n\n  .rlink{\n    padding: 1em;\n    display: flex;\n    justify-content: center;\n    column-gap: 3em;\n    border-bottom: 1px solid gainsboro;\n    text-decoration: none;\n    color: #2c3e50;\n  }\n  .rlink:hover{\n    background-color: gainsboro;\n  }\n  </style>\n"],"mappings":";AAsBA,OAAOA,IAAG,MAAO,OAAM;AACvB,SAASC,OAAM,QAAS,oBAAmB;AAC3C,SAASC,GAAG,EAAEC,cAAa,QAAS,mBAAkB;AAEtD,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,MAAM,EAAE;MACNC,IAAI,EAAEC;IACR,CAAC;IACDC,EAAE,EAAE;MACFF,IAAI,EAAEC;IACR;EACF,CAAC;EACDE,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,KAAK,EAAE;IACT;EACF,