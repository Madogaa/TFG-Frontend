{"ast":null,"code":"import http from 'axios';\nexport default {\n  name: 'SignUpView',\n  components: {},\n  data() {\n    return {\n      users: [],\n      userName: '',\n      password: '',\n      isBad: false,\n      msg: ''\n    };\n  },\n  methods: {\n    checkUser() {\n      return http({\n        method: 'GET',\n        url: 'http://localhost:8080/usuarios'\n      }).then(response => response.data).then(users => {\n        this.users = users.filter(user => user.nombre === this.userName);\n        if (!this.users.length) {\n          this.msg = 'Usuario no existe';\n          this.isBad = true;\n        } else {\n          if (users.password !== this.password) {\n            this.msg = 'Contraseña no coincide';\n            this.isBad = true;\n          } else {\n            this.isBad = false;\n          }\n        }\n      }).catch(err => console.log(err));\n    }\n  }\n};","map":{"version":3,"names":["http","name","components","data","users","userName","password","isBad","msg","methods","checkUser","method","url","then","response","filter","user","nombre","length","catch","err","console","log"],"sources":["C:\\Users\\mario\\OneDrive\\Escritorio\\DotShare-TFG\\dotshare\\src\\views\\SignIn.vue"],"sourcesContent":["<template>\r\n  <div class=\"signin\">\r\n      <p class=\"info\" v-if=\"isBad\"> {{ msg }}</p>\r\n      <div class=\"box\">\r\n          <h2>Register</h2>\r\n          <div class=\"form__input\">\r\n              <p>Usuario: </p>\r\n              <input v-model=\"userName\" type=\"text\" name=\"user\" id=\"\">\r\n          </div>\r\n          <div class=\"form__input\">\r\n              <p>Contraseña: </p>\r\n              <input v-model=\"password\" type=\"text\" name=\"password\" id=\"\">\r\n          </div>\r\n          <div>\r\n              <button v-on:click=\"this.checkUser\" class=\"submit\" type=\"submit\">Registrar</button>\r\n          </div>\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport http from 'axios'\r\nexport default {\r\n  name: 'SignUpView',\r\n  components: {\r\n  },\r\n  data () {\r\n    return {\r\n      users: [],\r\n      userName: '',\r\n      password: '',\r\n      isBad: false,\r\n      msg: ''\r\n    }\r\n  },\r\n  methods: {\r\n    checkUser () {\r\n      return http(\r\n        {\r\n          method: 'GET',\r\n          url: 'http://localhost:8080/usuarios'\r\n        }\r\n      )\r\n        .then((response) => response.data)\r\n        .then(users => {\r\n          this.users = users.filter(user => user.nombre === this.userName)\r\n          if (!this.users.length) {\r\n            this.msg = 'Usuario no existe'\r\n            this.isBad = true\r\n          } else {\r\n            if (users.password !== this.password) {\r\n              this.msg = 'Contraseña no coincide'\r\n              this.isBad = true\r\n            } else {\r\n              this.isBad = false\r\n            }\r\n          }\r\n        })\r\n        .catch((err) => console.log(err))\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n  .info{\r\n    width: 100%;\r\n    background-color: red;\r\n    color: white;\r\n    margin: 0;\r\n    padding: 10px 0;\r\n    font-weight: bold;\r\n  }\r\n</style>\r\n"],"mappings":"AAqBA,OAAOA,IAAG,MAAO,OAAM;AACvB,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE,CACZ,CAAC;EACDC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,KAAK;MACZC,GAAG,EAAE;IACP;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,SAAQA,CAAA,EAAK;MACX,OAAOV,IAAI,CACT;QACEW,MAAM,EAAE,KAAK;QACbC,GAAG,EAAE;MACP,EACF,CACGC,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACX,IAAI,EAChCU,IAAI,CAACT,KAAI,IAAK;QACb,IAAI,CAACA,KAAI,GAAIA,KAAK,CAACW,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACC,MAAK,KAAM,IAAI,CAACZ,QAAQ;QAC/D,IAAI,CAAC,IAAI,CAACD,KAAK,CAACc,MAAM,EAAE;UACtB,IAAI,CAACV,GAAE,GAAI,mBAAkB;UAC7B,IAAI,CAACD,KAAI,GAAI,IAAG;QAClB,OAAO;UACL,IAAIH,KAAK,CAACE,QAAO,KAAM,IAAI,CAACA,QAAQ,EAAE;YACpC,IAAI,CAACE,GAAE,GAAI,wBAAuB;YAClC,IAAI,CAACD,KAAI,GAAI,IAAG;UAClB,OAAO;YACL,IAAI,CAACA,KAAI,