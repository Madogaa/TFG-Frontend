{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport http from 'axios';\nexport default {\n  name: 'SignUpView',\n  components: {},\n  data() {\n    return {\n      user: {\n        id_usuario: 0,\n        nombre: '',\n        email: '',\n        passwd: ''\n      },\n      userName: '',\n      password: '',\n      isBad: false,\n      msg: ''\n    };\n  },\n  methods: {\n    checkUser() {\n      return http({\n        method: 'GET',\n        url: 'http://localhost:8080/usuarios'\n      }).then(response => response.data).then(users => {\n        this.user = users.filter(user => user.nombre === this.userName)[0];\n        if (!this.user) {\n          this.msg = 'Usuario no existe';\n          this.isBad = true;\n        } else {\n          if (this.user.passwd !== this.password) {\n            this.msg = 'Contraseña no coincide';\n            this.isBad = true;\n          } else {\n            console.log('Inicisaste sesion');\n            console.log('vue:' + this.user);\n            this.$router.push({\n              name: 'profile',\n              params: {\n                user: this.user.nombre\n              }\n            });\n            this.isBad = false;\n          }\n        }\n      }).catch(err => console.log(err));\n    }\n  }\n};","map":{"version":3,"names":["http","name","components","data","user","id_usuario","nombre","email","passwd","userName","password","isBad","msg","methods","checkUser","method","url","then","response","users","filter","console","log","$router","push","params","catch","err"],"sources":["C:\\Users\\mario\\OneDrive\\Escritorio\\DotShare-TFG\\dotshare\\src\\views\\SignIn.vue"],"sourcesContent":["<template>\r\n  <div class=\"signin\">\r\n      <p class=\"info\" v-if=\"isBad\"> {{ msg }}</p>\r\n      <div class=\"box\">\r\n          <h2>Register</h2>\r\n          <div class=\"form__input\">\r\n              <p>Usuario: </p>\r\n              <input v-model=\"userName\" type=\"text\" name=\"user\" id=\"\">\r\n     