{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport http from 'axios';\nimport { storage } from '@/vuefire/firebase';\nimport { ref, getDownloadURL } from '@firebase/storage';\nexport default {\n  name: 'ProfileSearch',\n  props: {\n    search: {\n      type: String\n    },\n    id: {\n      type: String\n    }\n  },\n  computed: {\n    async imageSrc() {\n      const url = await getImageUrl(this.imageId, this.imageName);\n      return url;\n    }\n  },\n  data() {\n    return {\n      views: []\n    };\n  },\n  mounted() {\n    this.getListView();\n  },\n  methods: {\n    getImageUrl(id, filename) {\n      getDownloadURL(ref(storage, `perfil/${id}/${filename}`)).then(url => {\n        return url;\n      });\n    },\n    getListView() {\n      http({\n        method: 'GET',\n        url: `http://localhost:8080/perfil/${this.id}`\n      }).then(response => response.data).then(response => {\n        http({\n          method: 'GET',\n          url: `http://localhost:8080/perfil/${response.idPerfil}/${this.search}`\n        }).then(response => response.data).then(response => {\n          response.forEach(e => {\n            http({\n              method: 'GET',\n              url: `http://localhost:8080/perfilbyid/${e.idPerfil}`\n            }).then(response => response.data).then(response => {\n              e.nick = response.nombre;\n              this.views.push(e);\n            });\n          });\n        });\n      });\n    }\n  }\n};","map":{"version":3,"names":["http","storage","ref","getDownloadURL","name","props","search","type","String","id","computed","imageSrc","url","getImageUrl","imageId","imageName","data","views","mounted","getListView","methods","filename","then","method","response","idPerfil","forEach","e","nick","nombre","push"],"sources":["C:\\Users\\mario\\OneDrive\\Escritorio\\DotShare-TFG\\dotshare\\src\\views\\ProfileListView.vue"],"sourcesContent":["<template>\r\n    <div class=\"container\">\r\n        <router-link v-for=\"view in views\" :key=\"view.idPerfil\" class=\"rlink\" :to=\"'/profile/' + view.nick\">\r\n            <img class=\"profile__img\" :src=\"imageSrc\" alt=\"\">\r\n            <div class=\"content\">\r\n                <h2>@{{ view.nick }}</h2>\r\n                <h5>{{ view.nombre }}</h5>\r\n                <p>{{ view.nseguidores }}</p>\r\n            </div>\r\n        </router-link>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport http from 'axios'\r\nimport { storage } from '@/vuefire/firebase'\r\nimport { ref, getDownloadURL } from '@firebase/storage'\r\n\r\nexport default {\r\n  name: 'ProfileSearch',\r\n  props: {\r\n    search: {\r\n      type: String\r\n    },\r\n    id: {\r\n      type: String\r\n    }\r\n  },\r\n  computed: {\r\n    async imageSrc() {\r\n      const url = await getImageUrl(this.imageId, this.imageName)\r\n      return url\r\n    }\r\n  },\r\n  data () {\r\n    return {\r\n      views: []\r\n    }\r\n  },\r\n  mounted () {\r\n    this.getListView()\r\n  },\r\n  methods: {\r\n    getImageUrl (id, filename) {\r\n      getDownloadURL(ref(storage, `perfil/${id}/${filename}`))\r\n        .then(url => {\r\n          return url\r\n        })\r\n    },\r\n    getListView () {\r\n      http(\r\n        {\r\n          method: 'GET',\r\n          url: `http://localhost:8080/perfil/${this.id}`\r\n        }\r\n      )\r\n        .then((response) => response.data)\r\n        .then(response => {\r\n          http(\r\n            {\r\n              method: 'GET',\r\n              url: `http://localhost:8080/perfil/${response.idPerfil}/${this.search}`\r\n            }\r\n          ).then((response) => response.data)\r\n            .then(response => {\r\n              response.forEach(e => {\r\n                http(\r\n                  {\r\n                    method: 'GET',\r\n                    url: `http://localhost:8080/perfilbyid/${e.idPerfil}`\r\n                  }\r\n                ).then((response) => response.data)\r\n                  .then(response => {\r\n                    e.nick = response.nombre\r\n                    this.views.push(e)\r\n                  })\r\n              })\r\n            })\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n  <style scoped>\r\n  .profile__img{\r\n    background: red;\r\n    width: 128px;\r\n    object-fit:cover;\r\n    border-radius: 50%;\r\n  }\r\n  .content{\r\n    text-align: left;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    width: 270px;\r\n  }\r\n\r\n  .rlink{\r\n    padding: 1em;\r\n    width: 100%;\r\n    display: flex;\r\n    justify-content: center;\r\n    column-gap: 3em;\r\n    border-bottom: 1px solid gainsboro;\r\n    text-decoration: none;\r\n    color: #2c3e50;\r\n  }\r\n  .rlink:hover{\r\n    color: #2c3e50;\r\n  }\r\n  </style>\r\n"],"mappings":";AAcA,OAAOA,IAAG,MAAO,OAAM;AACvB,SAASC,OAAM,QAAS,oBAAmB;AAC3C,SAASC,GAAG,EAAEC,cAAa,QAAS,mBAAkB;AAEtD,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,MAAM,EAAE;MACNC,IAAI,EAAEC;IACR,CAAC;IACDC,EAAE,EAAE;MACFF,IAAI,EAAEC;IACR;EACF,CAAC;EACDE,QAAQ,EAAE;IACR,MAAMC,QAAQA,CAAA,EAAG;MACf,MAAMC,GAAE,GAAI,MAAMC,WAAW,CAAC,IAAI,CAACC,OAAO,EAAE,IAAI,CAACC,SAAS;MAC1D,OAAOH,GAAE;IACX;EACF,CAAC;EACDI,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,OAAMA,CAAA,EAAK;IACT,IAAI,CAACC,WAAW,EAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACPP,WAAUA,CAAGJ,EAAE,EAAEY,QAAQ,EAAE;MACzBlB,cAAc,CAACD,GAAG,CAACD,OAAO,EAAG,UAASQ,EAAG,IAAGY,QAAS,EAAC,CAAC,EACpDC,IAAI,CAACV,GAAE,IAAK;QACX,OAAOA,GAAE;MACX,CAAC;IACL,CAAC;IACDO,WAAUA,CAAA,EAAK;MACbnB,IAAI,CACF;QACEuB,MAAM,EAAE,KAAK;QACbX,GAAG,EAAG,gCAA+B,IAAI,CAACH,EAAG;MAC/C,EACF,CACGa,IAAI,CAAEE,QAAQ,IAAKA,QAAQ,CAACR,IAAI,EAChCM,IAAI,CAACE,QAAO,IAAK;QAChBxB,IAAI,CACF;UACEuB,MAAM,EAAE,KAAK;UACbX,GAAG,EAAG,gCAA+BY,QAAQ,CAACC,QAAS,IAAG,IAAI,CAACnB,MAAO;QACxE,EACD,CAACgB,IAAI,CAAEE,QAAQ,IAAKA,QAAQ,CAACR,IAAI,EAC/BM,IAAI,CAACE,QAAO,IAAK;UAChBA,QAAQ,CAACE,OAAO,CAACC,CAAA,IAAK;YACpB3B,IAAI,CACF;cACEuB,MAAM,EAAE,KAAK;cACbX,GAAG,EAAG,oCA