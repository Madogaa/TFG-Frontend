{"ast":null,"code":"import http from 'axios';\nimport MyModal from './Modal.vue';\nimport { storage } from '@/vuefire/firebase';\nimport { ref, getDownloadURL } from '@firebase/storage';\nexport default {\n  name: 'SignUpView',\n  components: {\n    MyModal\n  },\n  props: {\n    id: {\n      type: String,\n      default: '',\n      required: true\n    }\n  },\n  data() {\n    return {\n      perfil: {\n        idPerfil: '',\n        nombre: '',\n        img: '',\n        imgBanner: '',\n        creationDate: ''\n      },\n      publicaciones: [],\n      asignaturas: [],\n      archivos: [],\n      url: '',\n      showModal: false\n    };\n  },\n  mounted() {\n    this.getProfile();\n    this.getPublicaciones();\n  },\n  methods: {\n    getProfile() {\n      return http({\n        method: 'GET',\n        url: `http://localhost:8080/perfil/${this.id}`\n      }).then(response => response.data).then(perfil => {\n        this.perfil = perfil;\n        this.perfil.creationDate = this.perfil.creationDate.slice(0, 10);\n        return http({\n          method: 'GET',\n          url: `http://localhost:8080/publicaciones/${this.perfil.idPerfil}`\n        }).then(response => response.data).then(publicaciones => {\n          this.publicaciones = publicaciones;\n          this.asignaturas[0] = publicaciones[0].idAsignatura.nombre;\n          for (let i = 0; i < publicaciones.length; i++) {\n            console.log(!this.asignaturas === undefined && this.asignaturas.includes(publicaciones[i].idAsignatura.nombre));\n            if (!this.asignaturas === undefined && !this.asignaturas.includes(publicaciones[i].idAsignatura.nombre)) {\n              this.asignaturas[i] = publicaciones[i].idAsignatura.nombre;\n            }\n            const imgRef = ref(storage, publicaciones[i].upload);\n            getDownloadURL(imgRef).then(url => {\n              this.archivos[i] = url;\n            });\n          }\n        }).catch(err => console.log(err));\n      }).catch(err => console.log(err));\n    },\n    getPublicaciones() {},\n    dialog() {\n      this.showModal = true;\n    },\n    cierra(info) {\n      this.showModal = info;\n    }\n  },\n  watch: {\n    url() {\n      console.log('url');\n    }\n  }\n};","map":{"version":3,"names":["http","MyModal","storage","ref","getDownloadURL","name","components","props","id","type","String","default","required","data","perfil","idPerfil","nombre","img","imgBanner","creationDate","publicaciones","asignaturas","archivos","url","showModal","mounted","getProfile","getPublicaciones","methods","method","then","response","slice","idAsignatura","i","length","console","log","undefined","includes","imgRef","upload","catch","err","dialog","cierra","info","watch"],"sources":["C:\\Users\\mario\\OneDrive\\Escritorio\\DotShare-TFG\\dotshare\\src\\views\\Profile.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <my-modal :idPerfil=\"perfil.idPerfil\" @url=\"(info) => this.url = info\" :user=\"id\" @cierra=\"cierra\" v-if=\"showModal\"></my-modal>\r\n    <div class=\"profile\">\r\n        <!-- Banner -->\r\n        <div class=\"banner\">\r\n            <div class=\"profile__img\">\r\n\r\n            </div>\r\n        </div>\r\n        <div class=\"underbanner\">\r\n            <p style=\"padding: 0; margin: 0;color: white;\">Hi {{ id }}</p>\r\n            <p style=\"padding: 0; margin: 0;color: white;\">Hi {{ perfil.nombre }}</p>\r\n            <p style=\"padding: 0; margin: 0;color: white;\">Hi {{ perfil.imgBanner }}</p>\r\n            <p style=\"padding: 0; margin: 0;color: white;\">Hi {{ perfil.img }}</p>\r\n            <p style=\"padding: 0; margin: 0;color: white;\">Hi {{ perfil.creationDate }}</p>\r\n          </div>\r\n        <div class=\"contenido\">\r\n          <button v-on:click=\"dialog\">+</button>\r\n          <div v-for=\"(publicacion,index) in publicaciones\" v-bind:key=\"publicacion.idPublicacion\">\r\n            <h1>{{ publicacion.idPublicacion }}</h1>\r\n            <h1>{{ publicacion.descripcion }}</h1>\r\n            <a :href=\"archivos[index]\" download>\r\n               <img :alt=\"publicacion.upload\" width=\"104\" height=\"142\">\r\n            </a>\r\n          </div>\r\n          <div v-for=\"asignatura in asignaturas\" v-bind:key=\"asignatura.idAsignatura\">\r\n            <h1>{{ asignatura }}</h1>\r\n          </div>\r\n        </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport http from 'axios'\r\nimport MyModal from './Modal.vue'\r\nimport { storage } from '@/vuefire/firebase'\r\nimport { ref, getDownloadURL } from '@firebase/storage'\r\n\r\nexport default {\r\n  name: 'SignUpView',\r\n  components: {\r\n    MyModal\r\n  },\r\n  props: {\r\n    id: {\r\n      type: String,\r\n      default: '',\r\n      required: true\r\n    }\r\n  },\r\n  data () {\r\n    return {\r\n      perfil: {\r\n        idPerfil: '',\r\n        nombre: '',\r\n        img: '',\r\n        imgBanner: '',\r\n        creationDate: ''\r\n      },\r\n      publicaciones: [],\r\n      asignaturas: [],\r\n      archivos: [],\r\n      url: '',\r\n      showModal: false\r\n    }\r\n  },\r\n  mounted () {\r\n    this.getProfile()\r\n    this.getPublicaciones()\r\n  },\r\n  methods: {\r\n    getProfile () {\r\n      return http(\r\n        {\r\n          method: 'GET',\r\n          url: `http://localhost:8080/perfil/${this.id}`\r\n        }\r\n      )\r\n        .then((response) => response.data)\r\n        .then(perfil => {\r\n          this.perfil = perfil\r\n          this.perfil.creationDate = this.perfil.creationDate.slice(0, 10)\r\n          return http(\r\n            {\r\n              method: 'GET',\r\n              url: `http://localhost:8080/publicaciones/${this.perfil.idPerfil}`\r\n            }\r\n          )\r\n            .then((response) => response.data)\r\n            .then(publicaciones => {\r\n              this.publicaciones = publicaciones\r\n              this.asignaturas[0] = publicaciones[0].idAsignatura.nombre\r\n              for (let i = 0; i < publicaciones.length; i++) {\r\n                console.log(!this.asignaturas === undefined && this.asignaturas.includes(publicaciones[i].idAsignatura.nombre))\r\n                if (!this.asignaturas === undefined && !this.asignaturas.includes(publicaciones[i].idAsignatura.nombre)) {\r\n                  this.asignaturas[i] = publicaciones[i].idAsignatura.nombre\r\n                }\r\n                const imgRef = ref(storage, publicaciones[i].upload)\r\n                getDownloadURL(imgRef)\r\n                  .then((url) => {\r\n                    this.archivos[i] = url\r\n                  })\r\n              }\r\n            })\r\n            .catch((err) => console.log(err))\r\n        })\r\n        .catch((err) => console.log(err))\r\n    },\r\n    getPublicaciones () {\r\n    },\r\n    dialog () {\r\n      this.showModal = true\r\n    },\r\n    cierra (info) {\r\n      this.showModal = info\r\n    }\r\n  },\r\n  watch: {\r\n    url () {\r\n      console.log('url')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.profile{\r\n    background-color: gainsboro;\r\n}\r\n.banner{\r\n    position: relative;\r\n    height: 25vh;\r\n    background-color: black;\r\n}\r\n.underbanner{\r\n    height: 7em;\r\n    background-color: blueviolet;\r\n    margin: 0 0 0 16em;\r\n}\r\n.profile__img{\r\n    position: absolute;\r\n    background-color: red;\r\n    min-height: 14em;\r\n    min-width: 14em;\r\n    border-radius: 50%;\r\n    bottom: -7em;\r\n    left: 2em;\r\n}\r\n.contenido{\r\nmin-height: 55vh;\r\nbackground-color: aqua;\r\n}\r\n</style>\r\n"],"mappings":"AAmCA,OAAOA,IAAG,MAAO,OAAM;AACvB,OAAOC,OAAM,MAAO,aAAY;AAChC,SAASC,OAAM,QAAS,oBAAmB;AAC3C,SAASC,GAAG,EAAEC,cAAa,QAAS,mBAAkB;AAEtD,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVL;EACF,CAAC;EACDM,KAAK,EAAE;IACLC,EAAE,EAAE;MACFC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE,EAAE;MACXC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,MAAM,EAAE;QACNC,QAAQ,EAAE,EAAE;QAC