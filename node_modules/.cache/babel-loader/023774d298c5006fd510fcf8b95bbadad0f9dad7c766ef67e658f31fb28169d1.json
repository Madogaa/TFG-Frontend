{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport http from 'axios';\nimport { storage } from '@/vuefire/firebase';\nimport { ref, getDownloadURL } from '@firebase/storage';\nexport default {\n  name: 'ProfileSearch',\n  props: {\n    search: {\n      type: String\n    },\n    id: {\n      type: String\n    }\n  },\n  data() {\n    return {\n      views: []\n    };\n  },\n  mounted() {\n    this.getListView();\n  },\n  methods: {\n    getImageUrl(view) {\n      getDownloadURL(ref(storage, `perfil/${view.id}/${view.img}`)).then(url => {\n        view.loaded = true;\n        view.imageUrl = url;\n      });\n    },\n    getListView() {\n      http({\n        method: 'GET',\n        url: `http://localhost:8080/perfil/${this.id}`\n      }).then(response => response.data).then(response => {\n        http({\n          method: 'GET',\n          url: `http://localhost:8080/perfil/${response.idPerfil}/${this.search}`\n        }).then(response => response.data).then(response => {\n          response.forEach(e => {\n            http({\n              method: 'GET',\n              url: `http://localhost:8080/perfilbyid/${e.idPerfil}`\n            }).then(response => response.data).then(response => {\n              e.nick = response.nombre;\n              e.loaded = false;\n              this.views.push(e);\n            });\n          }).then(e => {\n            this.views.forEach(view => {\n              this.getImageUrl(view);\n            });\n          });\n        });\n      });\n    }\n  }\n};","map":{"version":3,"names":["http","storage","ref","getDownloadURL","name","props","search","type","String","id","data","views","mounted","getListView","methods","getImageUrl","view","img","then","url","loaded","imageUrl","method","response","idPerfil","forEach","e","nick","nombre","push"],"sources":["C:\\Users\\mario\\OneDrive\\Escritorio\\DotShare-TFG\\dotshare\\src\\views\\ProfileListView.vue"],"sourcesContent":["<template>\r\n    <div class=\"container\">\r\n        <router-link v-for=\"view in views\" :key=\"view.idPerfil\" class=\"rlink\" :to=\"'/profile/' + view.nick\">\r\n            <img v-if=\"view.loaded\" class=\"profile__img\" :src=\"view.imageUrl\" alt=\"\">\r\n            <div class=\"content\">\r\n                <h2>@{{ view.nick }}</h2>\r\n                <h5>{{ view.nombre }}</h5>\r\n                <p>{{ view.nseguidores }}</p>\r\n            </div>\r\n        </router-link>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport http from 'axios'\r\nimport { storage } from '@/vuefire/firebase'\r\nimport { ref, getDownloadURL } from '@firebase/storage'\r\n\r\nexport default {\r\n  name: 'ProfileSearch',\r\n  props: {\r\n    search: {\r\n      type: String\r\n    },\r\n    id: {\r\n      type: String\r\n    }\r\n  },\r\n  data () {\r\n    return {\r\n      views: []\r\n    }\r\n  },\r\n  mounted () {\r\n    this.getListView()\r\n  },\r\n  methods: {\r\n    getImageUrl (view) {\r\n      getDownloadURL(ref(storage, `perfil/${view.id}/${view.img}`))\r\n        .then(url => {\r\n          view.loaded = true\r\n          view.imageUrl = url\r\n        })\r\n    },\r\n    getListView () {\r\n      http(\r\n        {\r\n          method: 'GET',\r\n          url: `http://localhost:8080/perfil/${this.id}`\r\n        }\r\n      )\r\n        .then((response) => response.data)\r\n        .then(response => {\r\n          http(\r\n            {\r\n              method: 'GET',\r\n              url: `http://localhost:8080/perfil/${response.idPerfil}/${this.search}`\r\n            }\r\n          ).then((response) => response.data)\r\n            .then(response => {\r\n              response.forEach(e => {\r\n                http(\r\n                  {\r\n                    method: 'GET',\r\n                    url: `http://localhost:8080/perfilbyid/${e.idPerfil}`\r\n                  }\r\n                ).then((response) => response.data)\r\n                  .then(response => {\r\n                    e.nick = response.nombre\r\n                    e.loaded = false\r\n                    this.views.push(e)\r\n                  })\r\n              }).then(e => {\r\n                this.views.forEach(view => {\r\n                  this.getImageUrl(view)\r\n                })\r\n              })\r\n            })\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n  <style scoped>\r\n  .profile__img{\r\n    background: red;\r\n    width: 128px;\r\n    object-fit:cover;\r\n    border-radius: 50%;\r\n  }\r\n  .content{\r\n    text-align: left;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    width: 270px;\r\n  }\r\n\r\n  .rlink{\r\n    padding: 1em;\r\n    width: 100%;\r\n    display: flex;\r\n    justify-content: center;\r\n    column-gap: 3em;\r\n    border-bottom: 1px solid gainsboro;\r\n    text-decoration: none;\r\n    color: #2c3e50;\r\n  }\r\n  .rlink:hover{\r\n    color: #2c3e50;\r\n  }\r\n  </style>\r\n"],"mappings":";AAcA,OAAOA,IAAG,MAAO,OAAM;AACvB,SAASC,OAAM,QAAS,oBAAmB;AAC3C,SAASC,GAAG,EAAEC,cAAa,QAAS,mBAAkB;AAEtD,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,MAAM,EAAE;MACNC,IAAI,EAAEC;IACR,CAAC;IACDC,EAAE,EAAE;MACFF,IAAI,EAAEC;IACR;EACF,CAAC;EACDE,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,OAAMA,CAAA,EAAK;IACT,IAAI,CAACC,WAAW,EAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACPC,WAAUA,CAAGC,IAAI,EAAE;MACjBb,cAAc,CAACD,GAAG,CAACD,OAAO,EAAG,UAASe,IAAI,CAACP,EAAG,IAAGO,IAAI,CAACC,GAAI,EAAC,CAAC,EACzDC,IAAI,CAACC,GAAE,IAAK;QACXH,IAAI,CAACI,MAAK,GAAI,IAAG;QACjBJ,IAAI,CAACK,QAAO,GAAIF,GAAE;MACpB,CAAC;IACL,CAAC;IACDN,WAAUA,CAAA,EAAK;MACbb,IAAI,CACF;QACEsB,MAAM,EAAE,KAAK;QACbH,GAAG,EAAG,gCAA+B,IAAI,CAACV,EAAG;MAC/C,EACF,CACGS,IAAI,CAAEK,QAAQ,IAAKA,QAAQ,CAACb,IAAI,EAChCQ,IAAI,CAACK,QAAO,IAAK;QAChBvB,IAAI,CACF;UACEsB,MAAM,EAAE,KAAK;UACbH,GAAG,EAAG,gCAA+BI,QAAQ,CAACC,QAAS,IAAG,IAAI,CAAClB,MAAO;QACxE,EACD,CAACY,IAAI,CAAEK,QAAQ,IAAKA,QAAQ,CAACb,IAAI,EAC/BQ,IAAI,CAACK,QAAO,IAAK;UAChBA,QAAQ,CAACE,OAAO,CAACC,CAAA,IAAK;YACpB1B,IAAI,CACF