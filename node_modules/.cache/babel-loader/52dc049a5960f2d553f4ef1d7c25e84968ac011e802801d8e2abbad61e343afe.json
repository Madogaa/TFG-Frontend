{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport http from 'axios';\nimport router from '@/router';\nexport default {\n  name: 'SignUpView',\n  components: {},\n  data() {\n    return {\n      users: [],\n      userName: '',\n      password: '',\n      isBad: false,\n      msg: ''\n    };\n  },\n  methods: {\n    checkUser() {\n      return http({\n        method: 'GET',\n        url: 'http://localhost:8080/usuarios'\n      }).then(response => response.data).then(users => {\n        this.users = users.filter(user => user.nombre === this.userName);\n        if (!this.users.length) {\n          this.msg = 'Usuario no existe';\n          this.isBad = true;\n        } else {\n          if (this.users[0].passwd !== this.password) {\n            this.msg = 'Contraseña no coincide';\n            this.isBad = true;\n          } else {\n            console.log('Inicisaste sesion');\n            router.push({\n              path: '/profile',\n              params: {\n                name: 'yeeeaa'\n              }\n            });\n            this.isBad = false;\n          }\n        }\n      }).catch(err => console.log(err));\n    }\n  }\n};","map":{"version":3,"names":["http","router","name","components","data","users","userName","password","isBad","msg","methods","checkUser","method","url","then","response","filter","user","nombre","length","passwd","console","log","push","path","params","catch","err"],"sources":["C:\\Users\\mario\\OneDrive\\Escritorio\\DotShare-TFG\\dotshare\\src\\views\\SignIn.vue"],"sourcesContent":["<template>\r\n  <div class=\"signin\">\r\n      <p class=\"info\" v-if=\"isBad\"> {{ msg }}</p>\r\n      <div class=\"box\">\r\n          <h2>Register</h2>\r\n          <div class=\"form__input\">\r\n              <p>Usuario: </p>\r\n              <input v-model=\"userName\" type=\"text\" name=\"user\" id=\"\">\r\n          </div>\r\n          <div class=\"form__input\">\r\n              <p>Contraseña: </p>\r\n              <input v-model=\"password\" type=\"text\" name=\"password\" id=\"\">\r\n          </div>\r\n          <div>\r\n              <button v-on:click=\"this.checkUser\" class=\"submit\" type=\"submit\">Registrar</button>\r\n          </div>\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport http from 'axios'\r\nimport router from '@/router'\r\nexport default {\r\n  name: 'SignUpView',\r\n  components: {\r\n  },\r\n  data () {\r\n    return {\r\n      users: [],\r\n      userName: '',\r\n      password: '',\r\n      isBad: false,\r\n      msg: ''\r\n    }\r\n  },\r\n  methods: {\r\n    checkUser () {\r\n      return http(\r\n        {\r\n          method: 'GET',\r\n          url: 'http://localhost:8080/usuarios'\r\n        }\r\n      )\r\n        .then((response) => response.data)\r\n        .then(users => {\r\n          this.users = users.filter(user => user.nombre === this.userName)\r\n          if (!this.users.length) {\r\n            this.msg = 'Usuario no existe'\r\n            this.isBad = true\r\n          } else {\r\n            if (this.users[0].passwd !== this.password) {\r\n              this.msg = 'Contraseña no coincide'\r\n              this.isBad = true\r\n            } else {\r\n              console.log('Inicisaste sesion')\r\n              router.push( { path: '/profile', params: { name: 'yeeeaa' } } )\r\n              this.isBad = false\r\n            }\r\n          }\r\n        })\r\n        .catch((err) => console.log(err))\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n  .info{\r\n    width: 100%;\r\n    background-color: red;\r\n    color: white;\r\n    margin: 0;\r\n    padding: 10px 0;\r\n    font-weight: bold;\r\n  }\r\n</style>\r\n"],"mappings":";AAqBA,OAAOA,IAAG,MAAO,OAAM;AACvB,OAAOC,MAAK,MAAO,UAAS;AAC5B,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE,CACZ,CAAC;EACDC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,KAAK;MACZC,GAAG,EAAE;IACP;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,SAAQA,CA