{"ast":null,"code":"import http from 'axios';\nimport { storage } from '../vuefire/firebase';\nimport { uploadBytes, getDownloadURL, ref } from 'firebase/storage';\nexport default {\n  name: 'ModalVue',\n  props: {\n    user: {\n      type: String,\n      required: true\n    },\n    idPerfil: {\n      type: Number,\n      required: true\n    }\n  },\n  data() {\n    return {\n      url: '',\n      imagen: null,\n      descripcion: '',\n      showSelected: false,\n      selectedAsignatura: false\n    };\n  },\n  mounted() {\n    this.getGrados();\n  },\n  methods: {\n    clickImagen(e) {\n      this.imagen = e.target.files[0];\n      console.log(this.imagen);\n    },\n    publicar() {\n      console.log(this.descripcion + '/' + this.imagen.name + '/' + this.selectGrado.idGrado + '/' + this.selectAsignatura.idAsignatura);\n      const imgRef = ref(storage, `publicacion/${this.user}/${this.imagen.name}`);\n      console.log(this.idPerfil);\n      uploadBytes(imgRef, this.imagen).then(e => console.log(e)).then(e => console.log(e));\n      // eslint-disable-next-line node/handle-callback-err\n      return http.post('http://localhost:8080/publicacion', {\n        descripcion: this.descripcion,\n        upload: `publicacion/${this.user}/${this.imagen.name}`,\n        idPerfil: this.idPerfil,\n        idAsignatura: this.selectAsignatura.idAsignatura\n      }).then(response => response.data).then(response => console.log(response)).then(this.close()).catch(err => console.log(err));\n    },\n    getImagen() {\n      const imgRef = ref(storage, 'images/laura/c2.png');\n      getDownloadURL(imgRef).then(url => {\n        console.log('emit' + url);\n      }).catch(error => {\n        switch (error.code) {\n          case 'storage/object-not-found':\n            break;\n          case 'storage/unauthorized':\n            break;\n          case 'storage/canceled':\n            break;\n          case 'storage/unknown':\n            break;\n        }\n      });\n    },\n    close() {\n      this.$emit('cierra', false);\n    }\n  }\n};","map":{"version":3,"names":["http","storage","uploadBytes","getDownloadURL","ref","name","props","user","type","String","required","idPerfil","Number","data","url","imagen","descripcion","showSelected","selectedAsignatura","mounted","getGrados","methods","clickImagen","e","target","files","console","log","publicar","selectGrado","idGrado","selectAsignatura","idAsignatura","imgRef","then","post","upload","response","close","catch","err","getImagen","error","code","$emit"],"sources":["C:\\Users\\mario\\OneDrive\\Escritorio\\DotShare-TFG\\dotshare\\src\\views\\ModalSetting.vue"],"sourcesContent":["<template>\r\n  <div class=\"modal show\" tabindex=\"-1\" role=\"dialog\">\r\n    <div class=\"modal-dialog\" role=\"document\">\r\n      <div class=\"modal-content\">\r\n        <div class=\"modal-header\">\r\n          <h5 class=\"modal-title\">Settings</h5>\r\n          <button @click=\"close\" type=\"button\" class=\"close\" aria-label=\"Close\">\r\n            <span aria-hidden=\"true\">&times;</span>\r\n          </button>\r\n        </div>\r\n        <form @submit.prevent=\"publicar\" enctype=\"multipart/form-data\">\r\n          <div class=\"modal-body\">\r\n            <div class=\"form__input\">\r\n              <p>Nombre: </p>\r\n              <input type=\"text\" v-model=\"nombre\" name=\"descripcion\" />\r\n            </div>\r\n            <div class=\"form__input-file\">\r\n              <p>Imagen Perfil: </p>\r\n              <input @change=\"clickImagen($event)\" type=\"file\" accept=\"application/msword, application/vnd.ms-excel, application/vnd.ms-powerpoint,text/plain, application/pdf, image/*\" />\r\n            </div>\r\n            <div class=\"form__input-file\">\r\n              <p>Banner: </p>\r\n              <input @change=\"clickImagen($event)\" type=\"file\" accept=\"application/msword, application/vnd.ms-excel, application/vnd.ms-powerpoint,text/plain, application/pdf, image/*\" />\r\n            </div>\r\n          </div>\r\n          <div class=\"modal-footer\">\r\n            <input type=\"submit\" class=\"btn btn-primary\" value=\"Save Changes\"/>\r\n            <button @click=\"close\" type=\"button\" class=\"btn btn-secondary\">Close</button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n  </div>\r\n</div></template>\r\n\r\n<script>\r\nimport http from 'axios'\r\nimport { storage } from '../vuefire/firebase'\r\nimport { uploadBytes, getDownloadURL, ref } from 'firebase/storage'\r\n\r\nexport default {\r\n  name: 'ModalVue',\r\n  props: {\r\n    user: {\r\n      type: String,\r\n      required: true\r\n    },\r\n    idPerfil: {\r\n      type: Number,\r\n      required: true\r\n    }\r\n  },\r\n  data () {\r\n    return {\r\n      url: '',\r\n      imagen: null,\r\n      descripcion: '',\r\n      showSelected: false,\r\n      selectedAsignatura: false\r\n    }\r\n  },\r\n  mounted () {\r\n    this.getGrados()\r\n  },\r\n  methods: {\r\n    clickImagen (e) {\r\n      this.imagen = e.target.files[0]\r\n      console.log(this.imagen)\r\n    },\r\n    publicar () {\r\n      console.log(this.descripcion + '/' + this.imagen.name + '/' + this.selectGrado.idGrado + '/' + this.selectAsignatura.idAsignatura)\r\n      const imgRef = ref(storage, `publicacion/${this.user}/${this.imagen.name}`)\r\n      console.log(this.idPerfil)\r\n      uploadBytes(imgRef, this.imagen).then(e => console.log(e))\r\n        .then(e => console.log(e))\r\n        // eslint-disable-next-line node/handle-callback-err\r\n      return http.post(\r\n        'http://localhost:8080/publicacion',\r\n        {\r\n          descripcion: this.descripcion,\r\n          upload: `publicacion/${this.user}/${this.imagen.name}`,\r\n          idPerfil: this.idPerfil,\r\n          idAsignatura: this.selectAsignatura.idAsignatura\r\n        }\r\n      )\r\n        .then((response) => response.data)\r\n        .then(response => console.log(response))\r\n        .then(this.close())\r\n        .catch((err) => console.log(err))\r\n    },\r\n    getImagen () {\r\n      const imgRef = ref(storage, 'images/laura/c2.png')\r\n      getDownloadURL(imgRef)\r\n        .then((url) => {\r\n          console.log('emit' + url)\r\n        })\r\n        .catch((error) => {\r\n          switch (error.code) {\r\n            case 'storage/object-not-found':\r\n              break\r\n            case 'storage/unauthorized':\r\n              break\r\n            case 'storage/canceled':\r\n              break\r\n            case 'storage/unknown':\r\n              break\r\n          }\r\n        })\r\n    },\r\n    close () {\r\n      this.$emit('cierra', false)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.show{\r\n  display: block;\r\n}\r\n.form-select{\r\n  margin: 0 0 1em 0;\r\n}\r\n</style>\r\n"],"mappings":"AAmCA,OAAOA,IAAG,MAAO,OAAM;AACvB,SAASC,OAAM,QAAS,qBAAoB;AAC5C,SAASC,WAAW,EAAEC,cAAc,EAAEC,GAAE,QAAS,kBAAiB;AAElE,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE;IACLC,IAAI,EAAE;MACJC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ,CAAC;IACDC,QAAQ,EAAE;MACRH,IAAI,EAAEI,MAAM;MACZF,QAAQ,EAAE;IACZ;EACF,CAAC;EACDG,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,GAAG,EAAE,EAAE;MACPC,MAAM,EAAE,IAAI;MACZC,WAAW,EAAE,EAAE;MACfC,YAAY,EAAE,KAAK;MACnBC,kBAAkB,EAAE;IACtB;EACF,CAAC;EACDC,OAAMA,CAAA,EAAK;IACT,IAAI,CAACC,SAAS,EAAC;EACjB,CAAC;EACDC,OAAO,EAAE;IACPC,WAAUA,CAAGC,CAAC,EAAE;MACd,IAAI,CAACR,MAAK,GAAIQ,CAAC,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC;MAC9BC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACZ,MAAM;IACzB,CAAC;IACDa,QAAOA,CAAA,EAAK;MACVF,OAAO,CAACC,GAAG,CAAC,IAAI,CAACX,WAAU,GAAI,GAAE,GAAI,IAAI,CAACD,MAAM,CAACV,IAAG,GAAI,GAAE,GAAI,IAAI,CAACwB,WAAW,CAACC,OAAM,GAAI,GAAE,GAAI,IAAI,CAACC,gBAAgB,CAACC,YAAY;MACjI,MAAMC,MAAK,GAAI7B,GAAG,CAACH,OAAO,EAAG,eAAc,IAAI,CAACM,IAAK,IAAG,IAAI,CAACQ,MAAM,CAACV,IAAK,EAAC;MAC1EqB,OAAO,CAACC,GAAG,CAAC,IAAI,CAAChB,QAAQ;MACzBT,WAAW,CAAC+B,MAAM,EAAE,IAAI,CAAClB,MAAM,CAAC,CAACmB,IAAI,CAACX,CAAA,IAAKG,OAAO,CAACC,GAAG,CAACJ,CAAC,CAAC,EACtDW,IAAI,CAACX,CAAA,IAAKG,OAAO,CAACC,GAAG,CAACJ