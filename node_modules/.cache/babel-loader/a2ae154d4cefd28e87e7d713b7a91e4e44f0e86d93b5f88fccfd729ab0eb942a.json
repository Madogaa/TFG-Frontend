seguidores"],"sources":["C:\\Users\\mario\\OneDrive\\Escritorio\\DotShare-TFG\\dotshare\\src\\views\\ProfileListView.vue"],"sourcesContent":["<template>\r\n    <div class=\"container\">\r\n        <router-link v-for=\"view in views\" :key=\"view.idPerfil\" class=\"rlink\" :to=\"'/profile/' + view.nick\">\r\n            <img class=\"profile__img\" :src=\"getImageUrl(view.nick,view.img)\" alt=\"\">\r\n            <div class=\"content\">\r\n                <h2>@{{ view.nick }}</h2>\r\n                <h5>{{ view.nombre }}</h5>\r\n                <p>{{ view.nseguidores }}</p>\r\n            </div>\r\n        </router-link>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport http from 'axios'\r\nimport { storage } from '@/vuefire/firebase'\r\nimport { ref, getDownloadURL } from '@firebase/storage'\r\n\r\nexport default {\r\n  name: 'ProfileSearch',\r\n  props: {\r\n    search: {\r\n      type: String\r\n    },\r\n    id: {\r\n      type: String\r\n    }\r\n  },\r\n  data () {\r\n    return {\r\n      views: []\r\n    }\r\n  },\r\n  mounted () {\r\n    this.getListView()\r\n  },\r\n  methods: {\r\n    async getImageUrl (id, filename) {\r\n      try {\r\n        const url = await getDownloadURL(ref(storage, `perfil/${id}/${filename}`))\r\n        return url\r\n      } catch (error) {\r\n        console.log(error)\r\n        return null\r\n        }\r\n    },\r\n    getListView () {\r\n      http(\r\n        {\r\n          method: 'GET',\r\n          url: `http://localhost:8080/perfil/${this.id}`\r\n        }\r\n      )\r\n        .then((response) => response.data)\r\n        .then(response => {\r\n          http(\r\n            {\r\n              method: 'GET',\r\n              url: `http://localhost:8080/perfil/${response.idPerfil}/${this.search}`\r\n            }\r\n          ).then((response) => response.data)\r\n            .then(response => {\r\n              response.forEach(e => {\r\n                http(\r\n                  {\r\n                    method: 'GET',\r\n                    url: `http://localhost:8080/perfilbyid/${e.idPerfil}`\r\n                  }\r\n                ).then((response) => response.data)\r\n                  .then(response => {\r\n                    e.nick = response.nombre\r\n                    this.views.push(e)\r\n                  })\r\n              })\r\n            })\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n  <style scoped>\r\n  .profile__img{\r\n    background: red;\r\n    width: 128px;\r\n    object-fit:cover;\r\n    border-radius: 50%;\r\n  }\r\n  .content{\r\n    text-align: left;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    width: 270px;\r\n  }\r\n\r\n  .rlink{\r\n    padding: 1em;\r\n    width: 100%;\r\n    display: flex;\r\n    justify-content: center;\r\n    column-gap: 3em;\r\n    border-bottom: 1px solid gainsboro;\r\n    text-decoration: none;\r\n    color: #2c3e50;\r\n  }\r\n  .rlink:hover{\r\n    color: #2c3e50;\r\n  }\r\n  </style>\r\n"],"mappings":";;;EACSA,KAAK,EAAC;AAAW;;;EAGTA,KAAK,EAAC;AAAS;;;uBAH5BC,mBAAA,CASM,OATNC,UASM,I,kBARFD,mBAAA,CAOcE,SAAA,QAAAC,WAAA,CAPcC,KAAA,CAAAC,KAAK,EAAbC,IAAI;yBAAxBC,YAAA,CAOcC,sBAAA;MAPsBC,GAAG,EAAEH,IAAI,CAACI,QAAQ;MAAEX,KAAK,EAAC,OAAO;MAAEY,EAAE,gBAAgBL,IAAI,CAACM;;wBAC1F,MAAwE,CAAxEC,mBAAA,CAAwE;QAAnEd,KAAK,EAAC,cAAc;QAAEe,GAAG,EAAEC,QAAA,CAAAC,WAAW,CAACV,IAAI,CAACM,IAAI,EAACN,IAAI,CAACW,GAAG;QAAGC,GAAG,EAAC;2CACrEL,mBAAA,CAIM,OAJNM,UAIM,GAHFN,mBAAA,CAAyB,YAArB,GAAC,GAAAO,gBAAA,CAAGd,IAAI,CAACM,IAAI,kBACjBC,mBAAA,CAA0B,YAAAO,gBAAA,CAAnBd,IAAI,CAACe,MAAM,kBAClBR,mBAAA,CAA6B,WAAAO,gBAAA,CAAvBd,IAAI,CAACgB,WAAW,iB"},"metadata":{},"sourceType":"module","externalDependencies":[]}