{"ast":null,"code":"import http from 'axios';\nimport MyModal from './Modal.vue';\nexport default {\n  name: 'SignUpView',\n  components: {\n    MyModal\n  },\n  props: {\n    id: {\n      type: String,\n      default: '',\n      required: true\n    }\n  },\n  data() {\n    return {\n      perfil: {\n        idPerfil: '',\n        nombre: '',\n        img: '',\n        imgBanner: '',\n        creationDate: ''\n      },\n      url: '',\n      showModal: false\n    };\n  },\n  computed: {\n    fullname() {\n      return this.id + ' ' + this.id;\n    }\n  },\n  mounted() {\n    this.getProfile();\n  },\n  methods: {\n    getProfile() {\n      return http({\n        method: 'GET',\n        url: `http://localhost:8080/perfil/${this.id}`\n      }).then(response => response.data).then(perfil => {\n        this.perfil = perfil;\n        this.perfil.creationDate = this.perfil.creationDate.slice(0, 10);\n      }).catch(err => console.log(err));\n    },\n    dialog() {\n      this.showModal = true;\n    },\n    cierra(info) {\n      this.showModal = info;\n    }\n  },\n  watch: {\n    url() {\n      console.log('url');\n    }\n  }\n};","map":{"version":3,"names":["http","MyModal","name","components","props","id","type","String","default","required","data","perfil","idPerfil","nombre","img","imgBanner","creationDate","url","showModal","computed","fullname","mounted","getProfile","methods","method","then","response","slice","catch","err","console","log","dialog","cierra","info","watch"],"sources":["C:\\Users\\mario\\OneDrive\\Escritorio\\DotShare-TFG\\dotshare\\src\\views\\Profile.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <my-modal :idPerfil=\"perfil.idPerfil\" @url=\"(info) => this.url = info\" :user=\"id\" @cierra=\"cierra\" v-if=\"showModal\"></my-modal>\r\n    <div class=\"profile\">\r\n        <!-- Banner -->\r\n        <div class=\"banner\">\r\n            <div class=\"profile__img\">\r\n\r\n            </div>\r\n        </div>\r\n        <div class=\"underbanner\">\r\n            <p style=\"padding: 0; margin: 0;color: white;\">Hi {{ id }}</p>\r\n            <p style=\"padding: 0; margin: 0;color: white;\">Hi {{ perfil.nombre }}</p>\r\n            <p style=\"padding: 0; margin: 0;color: white;\">Hi {{ perfil.imgBanner }}</p>\r\n            <p style=\"padding: 0; margin: 0;color: white;\">Hi {{ perfil.img }}</p>\r\n            <p style=\"padding: 0; margin: 0;color: white;\">Hi {{ perfil.creationDate }}</p>\r\n          </div>\r\n        <div class=\"contenido\">\r\n          <button v-on:click=\"dialog\">+</button>\r\n          <img v-if=\"url\" :src=\"url\" alt=\"\">\r\n        </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport http from 'axios'\r\nimport MyModal from './Modal.vue'\r\nexport default {\r\n  name: 'SignUpView',\r\n  components: {\r\n    MyModal\r\n  },\r\n  props: {\r\n    id: {\r\n      type: String,\r\n      default: '',\r\n      required: true\r\n    }\r\n  },\r\n  data () {\r\n    return {\r\n      perfil: {\r\n        idPerfil: '',\r\n        nombre: '',\r\n        img: '',\r\n        imgBanner: '',\r\n        creationDate: ''\r\n      },\r\n      url: '',\r\n      showModal: false\r\n    }\r\n  },\r\n  computed: {\r\n    fullname () {\r\n      return this.id + ' ' + this.id\r\n    }\r\n  },\r\n  mounted () {\r\n    this.getProfile()\r\n  },\r\n  methods: {\r\n    getProfile () {\r\n      return http(\r\n        {\r\n          method: 'GET',\r\n          url: `http://localhost:8080/perfil/${this.id}`\r\n        }\r\n      )\r\n        .then((response) => response.data)\r\n        .then(perfil => {\r\n          this.perfil = perfil\r\n          this.perfil.creationDate = this.perfil.creationDate.slice(0, 10)\r\n        })\r\n        .catch((err) => console.log(err))\r\n    },\r\n    dialog () {\r\n      this.showModal = true\r\n    },\r\n    cierra (info) {\r\n      this.showModal = info\r\n    }\r\n  },\r\n  watch: {\r\n    url () {\r\n      console.log('url')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.profile{\r\n    background-color: gainsboro;\r\n}\r\n.banner{\r\n    position: relative;\r\n    height: 25vh;\r\n    background-color: black;\r\n}\r\n.underbanner{\r\n    height: 7em;\r\n    background-color: blueviolet;\r\n    margin: 0 0 0 16em;\r\n}\r\n.profile__img{\r\n    position: absolute;\r\n    background-color: red;\r\n    min-height: 14em;\r\n    min-width: 14em;\r\n    border-radius: 50%;\r\n    bottom: -7em;\r\n    left: 2em;\r\n}\r\n.contenido{\r\nmin-height: 55vh;\r\nbackground-color: aqua;\r\n}\r\n</style>\r\n"],"mappings":"AA0BA,OAAOA,IAAG,MAAO,OAAM;AACvB,OAAOC,OAAM,MAAO,aAAY;AAChC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,KAAK,EAAE;IACLC,EAAE,EAAE;MACFC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE,EAAE;MACXC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,MAAM,EAAE;QACNC,QAAQ,EAAE,EAAE;QACZC,MAAM,EAAE,EAAE;QACVC,GAAG,EAAE,EAAE;QACPC,SAAS,EAAE,EAAE;QACbC,YAAY,EAAE;MAChB,CAAC;MACDC,GAAG,EAAE,EAAE;MACPC,SAAS,EAAE;IACb;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,QAAOA,CAAA,EAAK;MACV,OAAO,IAAI,CAACf,EAAC,GAAI,GAAE,GAAI,IAAI,CAACA,EAAC;IAC/B;EACF,CAAC;EACDgB,OAAMA,CAAA,EAAK;IACT,IAAI,CAACC,UAAU,EAAC;EAClB,CAAC;EACDC,OAAO,EAAE;IACPD,UAASA,CAAA,EAAK;MACZ,OAAOtB,IAAI,CACT;QACEwB,MAAM,EAAE,KAAK;QACbP,GAAG,EAAG,gCAA+B,IAAI,CAACZ,EAAG;MAC/C,EACF,CACGoB,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAAChB,IAAI,EAChCe,IAAI,CAACd,MAAK,IAAK;QACd,IAAI,CAACA,MAAK,GAAIA,MAAK;QACnB,IAAI,CAACA,MAAM,CAACK,YAAW,GAAI,IAAI,CAACL,MAAM,CAACK,YAAY,CAACW,KAAK,CAAC,CAAC,EAAE,EAAE;MACjE,CAAC,EACAC,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;IACpC,CAAC;IACDG,MAAKA,CAAA,EAAK;MACR,IAAI,CAACd,SAAQ,GAAI,IAAG;IACtB,CAAC;IACDe,MAAKA,CAAGC,IAAI,EAAE;MACZ,IAAI,CAAChB,SAAQ,GAAIgB,IAAG;IACtB;EACF,CAAC;EACDC,KAAK,EAAE;IACLlB,GAAEA,CAAA,EAAK;MACLa,OAAO,CAACC,GAAG,CAAC,KAAK;IACnB;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}