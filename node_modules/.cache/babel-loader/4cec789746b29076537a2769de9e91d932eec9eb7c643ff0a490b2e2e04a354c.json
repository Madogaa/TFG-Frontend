his.views.push(e)\r\n                      })\r\n                      .catch(error => {\r\n                        e.loaded = false\r\n                        console.log('Error al descargar imagen:', error)\r\n                        this.views.push(e)\r\n                      })\r\n                  })\r\n              })\r\n              })\r\n          })\r\n        })\r\n    }\r\n  },\r\n  watch: {\r\n    '$route.params': {\r\n      handler () {\r\n        this.views = []\r\n        this.getListView()\r\n      },\r\n      immediate: true\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.profile__img{\r\n  background: red;\r\n  width: 128px;\r\n  object-fit:cover;\r\n  border-radius: 50%;\r\n}\r\n.content{\r\n  text-align: left;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  width: 270px;\r\n}\r\n\r\n.rlink{\r\n  padding: 1em;\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: center;\r\n  column-gap: 3em;\r\n  border-bottom: 1px solid gainsboro;\r\n  text-decoration: none;\r\n  color: #2c3e50;\r\n}\r\n.rlink:hover{\r\n  color: #2c3e50;\r\n}\r\n</style>\r\n"],"mappings":";AAcA,OAAOA,IAAG,MAAO,OAAM;AAEvB,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,MAAM,EAAE;MACNC,IAAI,EAAEC;IACR;EACF,CAAC;EACDC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,WAAUA,CAAA,EAAK;MACbT,IAAI,CACF;QACEU,MAAM,EAAE,KAAK;QACbC,GAAG,EAAG,oCAAmC,IAAI,CAACR,MAAO;MACvD,EACF,CACGS,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACP,IAAI,EAChCM,IAAI,CAACC,QAAO,IAAK;QAChBA,QAAQ,CAACC,OAAO,CAACC,CAAA,IAAK;UACpBf,IAAI,CACF;YACEU,MAAM,EAAE,KAAK;YACbC,GAAG,EAAG,oCAAmCI,CAAC,CAACC,QAAS;UACtD,EACD,CAACJ,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACP,IAAI,EAC/BM,IAAI,CAACC,QAAO,IAAK;YAChBA,QAAQ,CAACC,OAAO,CAACC,CAAA,IAAK;cACtBf,IAAI,CACF;gBACEU,MAAM,EAAE,KAAK;gBACbC,GAAG,EAAG,oCAAmCI,CAAC,CAACC,QAAS;cACtD,EACD,CAACJ,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACP,IAAI,EAC/BM,IAAI,CAACC,QAAO,IAAK;gBAChBE,CAAC,CAACE,IAAG,GAAIJ,QAAQ,CAACK,MAAK;gBACvBH,CAAC,CAACI,