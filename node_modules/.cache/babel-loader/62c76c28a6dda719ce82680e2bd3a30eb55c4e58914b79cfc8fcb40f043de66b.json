etImageUrl(view.nick,view.img)\" alt=\"\">\r\n            <div class=\"content\">\r\n                <h2>@{{ view.nick }}</h2>\r\n                <h5>{{ view.nombre }}</h5>\r\n                <p>{{ view.nseguidores }}</p>\r\n            </div>\r\n        </router-link>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport http from 'axios'\r\nimport { storage } from '@/vuefire/firebase'\r\nimport { ref, getDownloadURL } from '@firebase/storage'\r\n\r\nexport default {\r\n  name: 'ProfileSearch',\r\n  props: {\r\n    search: {\r\n      type: String\r\n    },\r\n    id: {\r\n      type: String\r\n    }\r\n  },\r\n  data () {\r\n    return {\r\n      views: []\r\n    }\r\n  },\r\n  mounted () {\r\n    this.getListView()\r\n  },\r\n  methods: {\r\n    getImageUrl(id,filename) {\r\n        let out = ''\r\n      getDownloadURL(ref(storage, `perfil/${id}/${filename}`))\r\n        .then(url => { out = url})\r\n      return out\r\n    },\r\n    getListView () {\r\n      http(\r\n        {\r\n          method: 'GET',\r\n          url: `http://localhost:8080/perfil/${this.id}`\r\n        }\r\n      )\r\n        .then((response) => response.data)\r\n        .then(response => {\r\n          http(\r\n            {\r\n              method: 'GET',\r\n              url: `http://localhost:8080/perfil/${response.idPerfil}/${this.search}`\r\n            }\r\n          ).then((response) => response.data)\r\n            .then(response => {\r\n              response.forEach(e => {\r\n                http(\r\n                  {\r\n                    method: 'GET',\r\n                    url: `http://localhost:8080/perfilbyid/${e.idPerfil}`\r\n                  }\r\n                ).then((response) => response.data)\r\n                  .then(response => {\r\n                    e.nick = response.nombre\r\n                    this.views.push(e)\r\n                  })\r\n              })\r\n            })\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n  <style scoped>\r\n  .profile__img{\r\n    background: red;\r\n    width: 128px;\r\n    object-fit:cover;\r\n    border-radius: 50%;\r\n  }\r\n  .content{\r\n    text-align: left;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    width: 270px;\r\n  }\r\n\r\n  .rlink{\r\n    padding: 1em;\r\n    width: 100%;\r\n    display: flex;\r\n    justify-content: center;\r\n    column-gap: 3em;\r\n    border-bottom: 1px solid gainsboro;\r\n    text-decoration: none;\r\n    color: #2c3e50;\r\n  }\r\n  .rlink:hover{\r\n    color: #2c3e50;\r\n  }\r\n  </style>\r\n"],"mappings":";AAcA,OAAOA,IAAG,MAAO,OAAM;AACvB,SAASC,OAAM,QAAS,oBAAmB;AAC3C,SAASC,GAAG,EAAEC,cAAa,QAAS,mBAAkB;AAEtD,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,MAAM,EAAE;MACNC,IAAI,EAAEC;IACR,CAAC;IACDC,EAAE,EAAE;MACFF,IAAI,EAAEC;IACR;EACF,CAAC;EACDE,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,OAAMA,CAAA,EAAK;IACT,IAAI,CAACC,WAAW,EAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAACN,EAAE,EAACO,QAAQ,EAAE;MACrB,IAAIC,GAAE,GAAI,EAAC;MACbd,cAAc,CAACD,GAAG,CAACD,OAAO,EAAG,UAASQ,EAAG,IAAGO,QAAS,EAAC,CAAC,EACpDE,IAAI,CAACC,GAAE,IAAK;QAAEF,GAAE,GAAIE,GAAG;MAAA,CAAC;MAC3B,OAAOF,GAAE;IACX,CAAC;IACDJ,WAAUA,CAAA,EAAK;MACbb,IAAI,CACF;QACEoB,MAAM,EAAE,KAAK;QACbD,GAAG,EAAG,gCAA+B,IAAI,CAACV,EAAG;MAC/C,EACF,CACGS,IAAI,CAAEG,QAAQ,IAAKA,QAAQ,CAACX,IAAI,EAChCQ,IAAI,CAACG,QAAO,IAAK;QAChBrB,IAAI,CACF;UACEoB,MAAM,EAAE,KAAK;UACbD,GAAG,EAAG,gCAA+BE,QAAQ,CAACC,QAAS,IAAG,IAAI,CAAChB,MAAO;QACxE,EACD,CAACY,IAAI,CAAEG,QAAQ,IAAKA,QAAQ,CAACX,IAAI,EAC/BQ,IAAI,CAACG,QAAO,IAAK;UAChBA,QAAQ,CAACE,OAAO,CAACC,CAAA,IAAK;YACpBxB,IAAI,CACF;cACEoB,MAAM,EAAE,KAAK;cACbD,GAAG,EAAG,oCAAmCK,CAAC,CAACF,QAAS;YACtD,EACD,CAACJ,IAAI,CAAEG,QAAQ,IAAKA,QAAQ,CAACX,IAAI,EAC/BQ,IAAI,CAACG,QAAO,IAAK;cAChBG,CAAC,CAACC,IAAG,GAAIJ,QAAQ,CAACK,MAAK;cACvB,IAAI,CAACf,KAAK,CAACgB,IAAI,CAACH,CAAC;YACnB,CAAC;UACL,CAAC;QACH,CAAC;MACL,CAAC;IACL;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}