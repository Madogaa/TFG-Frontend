{"ast":null,"code":"import { storage } from '@/vuefire/firebase';\nimport { ref, getDownloadURL } from '@firebase/storage';\nimport { saveAs } from 'file-saver';\nexport default {\n  name: 'MyPublicacion',\n  props: {\n    desc: {\n      type: String,\n      required: true\n    },\n    archivos: {\n      type: Array\n    },\n    profile: {\n      type: String\n    }\n  },\n  methods: {\n    downloadFile(info) {\n      console.log(info);\n      const storageRef = ref(storage, `publicacion/${this.profile}/${info.dir}`);\n      getDownloadURL(storageRef).then(url => {\n        fetch(url, {\n          method: 'GET',\n          mode: 'no-cors'\n        }).then(response => response.blob()).then(blob => {\n          saveAs(blob, `${info.dir}`);\n        });\n      });\n    }\n  }\n};","map":{"version":3,"names":["storage","ref","getDownloadURL","saveAs","name","props","desc","type","String","required","archivos","Array","profile","methods","downloadFile","info","console","log","storageRef","dir","then","url","fetch","method","mode","response","blob"],"sources":["C:\\Users\\mario\\OneDrive\\Escritorio\\DotShare-TFG\\dotshare\\src\\views\\Publicacion.vue"],"sourcesContent":["<template>\r\n    <div class=\"publicacion\">\r\n      <p class=\"desc\">\r\n        {{ desc }}\r\n      </p>\r\n      <div v-for=\"archivo in this.archivos\" v-bind:key=\"archivo.idArchivo\" @click=\"downloadFile(archivo)\" class=\"file\">\r\n        <p>{{ archivo.dir }}</p>\r\n      </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { storage } from '@/vuefire/firebase'\r\nimport { ref, getDownloadURL } from '@firebase/storage'\r\nimport { saveAs } from 'file-saver'\r\n\r\nexport default {\r\n  name: 'MyPublicacion',\r\n  props: {\r\n    desc: {\r\n      type: String,\r\n      required: true\r\n    },\r\n    archivos: {\r\n      type: Array\r\n    },\r\n    profile: {\r\n      type: String\r\n    }\r\n  },\r\n  methods: {\r\n    downloadFile (info) {\r\n      console.log(info)\r\n      const storageRef = ref(storage, `publicacion/${this.profile}/${info.dir}`)\r\n      getDownloadURL(storageRef)\r\n        .then(url => {\r\n          fetch(url,\r\n            {\r\n              method: 'GET',\r\n              mode: 'no-cors'\r\n            })\r\n            .then(response => response.blob())\r\n            .then(blob => {\r\n              saveAs(blob, `${info.dir}`)\r\n            })\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\na{\r\n  text-decoration: none;\r\n  color: black;\r\n}\r\np{\r\n  margin: 0;\r\n}\r\n.publicacion{\r\n  background: linear-gradient(60deg,rgba(255,255,255,0.13),rgba(52, 72, 94, 0.6));\r\n  border: solid 1px rgba(255, 255, 255, 0.2);\r\n  border-radius: 0.5em;\r\n  box-shadow: 0 0 1em rgba(255, 255, 255, 0.13);\r\n  margin: 25px 0;\r\n  color: white;\r\n  padding: 2em;\r\n  text-align: left;\r\n}\r\n.desc {\r\n  padding: 0.5em;\r\n}\r\n\r\n.file{\r\n  width: fit-content;\r\n  padding: 0 1em 0 2em;\r\n  position: relative;\r\n  border: solid 1px rgba(255, 255, 255, 0.2);\r\n  border-radius: 1em;\r\n  max-width: 150px;\r\n  margin: 0.5em;\r\n  cursor: pointer;\r\n}\r\n\r\n.file p{\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n  text-overflow: ellipsis;\r\n}\r\n.file::before{\r\n  content:\"\";\r\n  position: absolute;\r\n  background: greenyellow;\r\n  height: 0.5em;\r\n  width: 0.5em;\r\n  left: 1em;\r\n  bottom: 0.5em;\r\n  border-radius: 50%;\r\n}\r\n</style>\r\n"],"mappings":"AAYA,SAASA,OAAM,QAAS,oBAAmB;AAC3C,SAASC,GAAG,EAAEC,cAAa,QAAS,mBAAkB;AACtD,SAASC,MAAK,QAAS,YAAW;AAElC,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,IAAI,EAAE;MACJC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ,CAAC;IACDC,QAAQ,EAAE;MACRH,IAAI,EAAEI;IACR,CAAC;IACDC,OAAO,EAAE;MACPL,IAAI,EAAEC;IACR;EACF,CAAC;EACDK,OAAO,EAAE;IACPC,YAAWA,CAAGC,IAAI,EAAE;MAClBC,OAAO,CAACC,GAAG,CAACF,IAAI;MAChB,MAAMG,UAAS,GAAIjB,GAAG,CAACD,OAAO,EAAG,eAAc,IAAI,CAACY,OAAQ,IAAGG,IAAI,CAACI,GAAI,EAAC;MACzEjB,cAAc,CAACgB,UAAU,EACtBE,IAAI,CAACC,GAAE,IAAK;QACXC